<div class="center container">
  <h1 class="h1 mt4">My Channels</h1>
  <hr>
</div>

<div class="clearfix px4">
  <% @channels.each do |channel| %>
    <%= render(partial: 'channels/channel', locals: { channel: channel }) %>
  <% end %>
</div>

<% if user_signed_in? %>
  <div class="center clearfix mt4">
    <%= link_to('Create a channel', new_channel_path, class: "button button-outline black") %>
  </div>
<% end %>
